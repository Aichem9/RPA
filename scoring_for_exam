import numpy as np

N = int(input("배점 총 합 :"))
n = int(input("총 문항 수 :"))
score = list(map(float, input("문항 배점 리스트(2,3,4,5,6과 같이 수로만 입력해주세요.) :").split(",")))

for s in range(2,n-1):
  for w in range(2,n-1):
    for z in range(2,n-1):
      for y in range(2,n-1):
        for x in range(2,n-1):
          if (x+y+z+w+s == n)&(score[0]*x+score[1]*y+score[2]*z+score[3]*w+score[4]*s== N)&(z>=np.floor(n/5)) :
            print(x,y,z,w,s)

          else :
            continue

#sample
# 3.6, 3.8, 4.0, 4.2, 4.4
# 4.1, 4.3, 4.5, 4.7, 4.9
